{{else}}
import { {{sevicePageClass}} } from "src/services/data/{{unixPathCase serviceFile}}";
{{/if}}

const logger: Logger = Utils.getLogger("{{> applicationName }}.actions{{packageCase actionDirectory}}{{packageCase creatPathAction}}.{{snakeCase actionClass}}");

/**
 * {{actionClassDescription}}
 */
export class {{camelCase actionClass}} extends RouteActionService<any, {{seviceDataClass}}> {
